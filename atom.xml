---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>redengine.tv</title>
  <link href="http://redengine.tv/"/>
  <link type="application/atom+xml" rel="self" href="http://redengine.tv/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://redengine.tv/</id>
  <author>
    <name>Mark Bramley</name>
    <email>mark.bramley@redengine.tv</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://redengine.tv{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://redengine.tv{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>{{ post.posted_by }}</name>
      <uri>http://redengine.tv/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>